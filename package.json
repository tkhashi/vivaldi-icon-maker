    "ui:dev": "vite",
    "ui:build": "vite build",
    "ui:preview": "vite preview"
  },
  "keywords": [
    "vivaldi",
    "svg",
    "icon",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "typescript": "^5.4.0",
    "vite": "^5.4.8"
  }
}
