{
  "name": "vivaldi-icon-maker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "CLI tool to recolor Vivaldi SVG icons",
  "main": "dist/index.js",
  "bin": {
    "vivaldi-icon-maker": "dist/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit",
